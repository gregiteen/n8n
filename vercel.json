{
  "version": 2,
  "name": "n8n-ai-platform",
  "builds": [
    {
      "src": "packages/user-interface/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "packages/orchestrator/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/orchestrator/(.*)",
      "dest": "/packages/orchestrator/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/user-interface/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key",
    "ANTHROPIC_API_KEY": "@anthropic-api-key",
    "GOOGLE_API_KEY": "@google-api-key",
    "OPENROUTER_API_KEY": "@openrouter-api-key",
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_ANON_KEY": "@supabase-anon-key",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase-service-role-key",
    "DATABASE_URL": "@database-url",
    "NEXTAUTH_SECRET": "@nextauth-secret",
    "NEXTAUTH_URL": "@nextauth-url"
  },
  "functions": {
    "packages/orchestrator/src/server.js": {
      "maxDuration": 30
    }
  }
}
